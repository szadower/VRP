{
  "extends": "tslint:latest",
  "rules": {
      "interface-name": [true, "never-prefix"],
      "whitespace": [true, "check-branch", "check-operator"],
      "object-literal-sort-keys": false,
      "arrow-parens": false,
      "max-classes-per-file": false,
      "curly": [true, "ignore-same-line"]
  }
}
